@{
  Layout = "_Layout";
  ViewBag.Title = "Home";
}

@model Dictionary<int, string>;

<h1>Welcome to Della's Treat Tracker!</h1>

@foreach (var name in @Model.Values)
{
  <p>@name</p>
}

<hr>
<p>@Html.ActionLink("My Flavors", "Index", "Flavors")</p>
<p>@Html.ActionLink("My Treats", "Index", "Treats")</p>
<p>@Html.ActionLink("Log in", "Login", "Account")</p>
<p>@Html.ActionLink("Register New Account", "Index", "Account")</p>